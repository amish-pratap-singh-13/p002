{
  "manifest_version": 3,
  "name": "Job Skimmer",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "scripting"],
  "host_permissions": ["https://wellfound.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://wellfound.com/jobs*"],
      "js": ["content.js"]
    }
  ]
}
